<tool id="echo_main_cluster" name="Echo on cluster">
    <hidden>True</hidden>
    <command>echo "$echo" &gt; "$output"</command>
    <inputs>
        <param name="echo" type="text" title="Text to echo"/>
    </inputs>
    <outputs>
        <data name="output" format="txt" />
    </outputs>
</tool>
